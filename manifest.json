{
  "manifest_version": 3,
  "name": "Youtube playlist sort",
  "version": "0.1",
  "description": "Sorts playlist items in youtube",
  "permissions": ["storage", "tabs"],
  "content_scripts": [
    {
      "matches": ["*youtube.com/*"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ],
  "action": {
    "default_popup": "entrypoints/popup/index.html"
  }
}
